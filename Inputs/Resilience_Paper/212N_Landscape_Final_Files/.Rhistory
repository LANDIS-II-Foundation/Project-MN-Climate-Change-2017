fire_dat<- read.csv("K:/Research_Faculty/AFRI_Chippewa_Project/Output_From_Sims/Results_040616_FireCalibration/Rep2/dynamic-fire-summary-log.csv")
fire_dat <- fire_dat[,1:4]
head(fire_dat)
avg_fire_sizes <- fire_dat[,3]/fire_dat[,4]
all.firedat <- cbind(fire_dat, avg_fire_sizes)
all.firedat <- all.firedat[complete.cases(all.firedat),]
fire.regions <- unique(all.firedat[,2])
#Manually adjust to years the sim ran and the resolution of your cell size.
sim_years<-100
cells_per_ha<-4
fire.stats<- NULL
for (i in fire.regions){
fire.eco <- subset(all.firedat, all.firedat$FireRegion == i)
area.sum <- sum(fire.eco$TotalBurnedSites)
avg.area <-area.sum/sim_years ##avg sites burned per yer
tot.fires<- sum(fire.eco$NumberFires)
avg.fires <- tot.fires/sim_years  ##average # of fires per year
fire.max <- max(fire.eco$TotalBurnedSites)
fire.mean <- mean(fire.eco$avg_fire_sizes)
fire.stat<-c(avg.fires*cells_per_ha, avg.area*cells_per_ha, fire.max*cells_per_ha, fire.mean*cells_per_ha, i) #convert from sites to hectares
fire.stats<-rbind(fire.stats, fire.stat)
}
colnames(fire.stats) <- c("AVG num of fires/yr", "AVG ha burned/yr",
"Max sites burned", "Mean ha/fire", "Fire Region")
fire.stats
